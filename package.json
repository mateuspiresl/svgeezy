{
  "name": "svgeezy",
  "version": "1.0.0",
  "description": "A simple JavaScript plugin which detects SVG images on your website, and automatically looks for a standard image fallback for those older, less capable browsers.",
  "repository": {
    "type": "git",
    "url": "git://github.com/benhowdle89/svgeezy"
  },
  "main": "svgeezy.js",
  "scripts": {
    "minify": "uglifyjs -c -m --comments all -o svgeezy.min.js -- svgeezy.js"
  },
  "keywords": [
    "svg",
    "fallback",
    "png",
    "javascript"
  ],
  "contributors": [
    "Ben Howdle",
    "Jack Smith"
  ],
  "bugs": {
    "url": "https://github.com/benhowdle89/svgeezy/issues"
  },
  "devDependencies": {
    "uglify-js": "^3.4.0"
  }
}
